{
  "family": "todo-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::060570890627:role/ecsTaskExecutionRole",
  "containerDefinitions": [{
    "name": "todo-container",
    "image": "060570890627.dkr.ecr.us-west-1.amazonaws.com/todo:${imageTag}",
    "essential": true,
    "portMappings": [{
      "containerPort": 8000,
      "hostPort": 8000
    }]
  }]
}
